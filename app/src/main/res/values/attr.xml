<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="firstColor" format="color" />
    <attr name="secondColor" format="color" />
    <attr name="circleWidth" format="dimension" />
    <attr name="dotCount" format="integer" />
    <attr name="splitSize" format="integer|dimension" />
    <attr name="textSize" format="integer|dimension" />
    <attr name="bg" format="reference"></attr>

    <declare-styleable name="RoyoleVolumControlBar">
        <attr name="firstColor" />
        <attr name="secondColor" />
        <attr name="circleWidth" />
        <attr name="dotCount" />
        <attr name="splitSize" />
        <attr name="bg" />
        <attr name="textSize"/>
    </declare-styleable>

    <declare-styleable name="CellView">
        <attr name="item_type">
            <enum name="type_1" value="1"/>
            <enum name="type_2" value="2"/>
            <enum name="type_3" value="3"/>
        </attr>
    </declare-styleable>


</resources>