<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carldemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.carldemo.MainActivty"
            android:process="com.royole.setting"
            android:configChanges="keyboard|keyboardHidden|orientation|locale"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.carldemo.audioplayer.AudioListActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|locale"
            android:label="@string/title_audioplayer"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.carldemo.audioplayer.AudioDetailActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|locale"
            android:label="@string/title_audioplayer"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.carldemo.pickerview.PickerViewActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|locale"
            android:label="@string/title_pickerview"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.carldemo.circledial.CircleActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|locale"
            android:label="@string/title_circledial"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.carldemo.circledial.DialActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|locale"
            android:label="@string/title_circledial"
            android:screenOrientation="portrait" >
        </activity>
        
         <activity
            android:name="com.example.carldemo.bitmapmanager.BitmapManagerTestActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|locale"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.carldemo.view.TestViewActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|locale"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.carldemo.TestActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|locale"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>